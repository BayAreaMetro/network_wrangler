{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "features"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "FeatureCollection"
      ],
      "pattern": "^(.*)$"
    },
    "features": {
      "$id": "#/properties/features",
      "type": "array",
      "title": "The Features Schema",
      "items": {
        "$id": "#/properties/features/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "shstRefId",
          "shstGeometryId",
          "access",
          "area",
          "highway",
          "lanes",
          "distance",
          "maxspeed",
          "name",
          "oneway",
          "osm_link_id",
          "u",
          "v",
          "lanes",
          "transit_access",
          "drive_access",
          "walk_access",
          "bike_access",
          "locationReferences"
        ],
        "properties": {
          "shstRefId": {
            "$id": "#/properties/features/items/properties/shstRefId",
            "type": ["string", "null"],
            "title": "The shstRefId Schema",
            "default": "",
            "examples": [
              "6a22969708104ae2497244f3d079381d"
            ],
            "pattern": "^(.*)$"
          },
          "shstGeometryId": {
            "$id": "#/properties/features/items/properties/shstGeometryId",
            "type": ["string", "null"],
            "title": "The shstGeometryId Schema",
            "default": "",
            "examples": [
              "4a454269d65c6619a2d7e2026295a295"
            ],
            "pattern": "^(.*)$"
          },
          "access": {
            "$id": "#/properties/features/items/properties/access",
            "type": ["string", "null"],
            "title": "The Access Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "area": {
            "$id": "#/properties/features/items/properties/area",
            "type": ["string", "null"],
            "title": "The Area Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "bridge": {
            "$id": "#/properties/features/items/properties/bridge",
            "type": ["string", "null"],
            "title": "The Bridge Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "highway": {
            "$id": "#/properties/features/items/properties/highway",
            "type": ["string", "null"],
            "title": "The Highway Schema",
            "default": "",
            "examples": [
              "residential"
            ],
            "pattern": "^(.*)$"
          },
          "junction": {
            "$id": "#/properties/features/items/properties/junction",
            "type": ["string", "null"],
            "title": "The Junction Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "key": {
            "$id": "#/properties/features/items/properties/key",
            "type": ["number", "null"],
            "title": "The Key Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "lanes": {
            "$id": "#/properties/features/items/properties/]lanes",
            "type": ["number", "null"],
            "title": "The Osmlanes Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "distance": {
            "$id": "#/properties/features/items/properties/distance",
            "type": ["number", "null"],
            "title": "The Length Schema",
            "default": 0.0,
            "examples": [
              80.935
            ]
          },
          "maxspeed": {
            "$id": "#/properties/features/items/properties/maxspeed",
            "type": ["string", "null"],
            "title": "The Maxspeed Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "name": {
            "$id": "#/properties/features/items/properties/name",
            "type": ["string", "null"],
            "title": "The Name Schema",
            "default": "",
            "examples": [
              "Arkwright Street,North Rivoli Street"
            ],
            "pattern": "^(.*)$"
          },
          "oneway": {
            "$id": "#/properties/features/items/properties/oneway",
            "type": ["string", "null"],
            "title": "The Oneway Schema",
            "default": "",
            "examples": [
              "False"
            ],
            "pattern": "^(.*)$"
          },
          "osm_link_id": {
            "$id": "#/properties/features/items/properties/osm_link_id",
            "type": ["string", "null"],
            "title": "The osm_link_id Schema",
            "default": "",
            "examples": [
              "18214905,18220565"
            ],
            "pattern": "^(.*)$"
          },
          "ref": {
            "$id": "#/properties/features/items/properties/ref",
            "type": ["string", "null"],
            "title": "The Ref Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "service": {
            "$id": "#/properties/features/items/properties/service",
            "type": ["string", "null"],
            "title": "The Service Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "tunnel": {
            "$id": "#/properties/features/items/properties/tunnel",
            "type": ["string", "null"],
            "title": "The Tunnel Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "u": {
            "$id": "#/properties/features/items/properties/u",
            "type": "string",
            "title": "The U Schema",
            "default": "",
            "examples": [
              "493882040"
            ],
            "pattern": "^(.*)$"
          },
          "v": {
            "$id": "#/properties/features/items/properties/v",
            "type": "string",
            "title": "The V Schema",
            "default": "",
            "examples": [
              "187879468"
            ],
            "pattern": "^(.*)$"
          },
          "width": {
            "$id": "#/properties/features/items/properties/width",
            "type": ["string", "null"],
            "title": "The Width Schema",
            "default": "",
            "examples": [
              ""
            ],
            "pattern": "^(.*)$"
          },
          "model_link_id": {
            "$id": "#/properties/features/items/properties/model_link_id",
            "type": "integer",
            "title": "The Link_id Schema",
            "default": 0,
            "examples": [
              224
            ]
          },
          "transit_access": {
            "$id": "#/properties/features/items/properties/transit_access",
            "type": "integer",
            "title": "The transit_access Schema",
            "default": 0,
            "examples": [
              0
            ]
          },
          "drive_access": {
            "$id": "#/properties/features/items/properties/drive_access",
            "type": "integer",
            "title": "The drive_access Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "walk_access": {
            "$id": "#/properties/features/items/properties/walk_access",
            "type": "integer",
            "title": "The walk_access Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "bike_access": {
            "$id": "#/properties/features/items/properties/bike_access",
            "type": "integer",
            "title": "The bike_access Schema",
            "default": 0,
            "examples": [
              1
            ]
          },
          "locationReferences": {
            "$id": "#/properties/features/items/properties/locationReferences",
            "type": "array",
            "title": "The Locationreferences Schema",
            "items": {
              "$id": "#/properties/features/items/properties/locationReferences/items",
              "type": "object",
              "title": "The Items Schema",
              "required": [
                "sequence",
                "point",
                "intersectionId"
              ],
              "properties": {
                "sequence": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/sequence",
                  "type": "integer",
                  "title": "The Sequence Schema",
                  "default": 0,
                  "examples": [
                    1
                  ]
                },
                "point": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/point",
                  "type": "array",
                  "title": "The Point Schema",
                  "items": {
                    "$id": "#/properties/features/items/properties/locationReferences/items/properties/point/items",
                    "type": "number",
                    "title": "The Items Schema",
                    "default": 0.0,
                    "examples": [
                      -93.0837695,
                      44.963344
                    ]
                  }
                },
                "distanceToNextRef": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/distanceToNextRef",
                  "type": "number",
                  "title": "The Distancetonextref Schema",
                  "default": 0.0,
                  "examples": [
                    80.9850385918
                  ]
                },
                "bearing": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/bearing",
                  "type": "integer",
                  "title": "The Bearing Schema",
                  "default": 0,
                  "examples": [
                    0
                  ]
                },
                "intersectionId": {
                  "$id": "#/properties/features/items/properties/locationReferences/items/properties/intersectionId",
                  "type": ["string", "null"],
                  "title": "The Intersectionid Schema",
                  "default": "",
                  "examples": [
                    "4d0231aa0ebb779f142c2518703ee481"
                  ],
                  "pattern": "^(.*)$"
                }
              }
            }
          }
        }
      }
    }
  }
}
